{
    "builds": [
      {
        "src": "wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "wsgi.py"
      }
    ],
    "functions":{
      "wsgi.py": {
        "maxDuration": 300,
        "memory": 1024
      }
    },
    "runtime": "python3.9"

  }